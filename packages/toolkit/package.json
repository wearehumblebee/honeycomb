{
  "name": "@humblebee/toolkit",
  "description": "Tooling set for humblebee applications",
  "version": "0.1.0-beta.3",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "humblebee-toolkit": "bin/cli.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "tools",
    "build",
    "test"
  ],
  "license": "MIT",
  "author": {
    "name": "Humblebee AB",
    "email": "hello@humblebee.se",
    "url": "https://www.humblebee.se/"
  },
  "homepage": "https://github.com/wearehumblebee/honeycomb/packages/toolkit",
  "repository": {
    "type": "git",
    "url": "https://github.com/wearehumblebee/honeycomb.git",
    "directory": "packages/toolkit"
  },
  "scripts": {
    "cli": "humblebee-toolkit",
    "build": "yarn run build:package && yarn run build:cli",
    "build:package": "microbundle --no-compress --tsconfig tsconfig.build.json",
    "build:cli": "microbundle -i src/cli.ts -o dist/cli.js -f cjs --no-pkg-main --no-compress --tsconfig tsconfig.build.json",
    "dev": "microbundle --watch --no-compress --tsconfig tsconfig.build.json",
    "lint": "eslint \"{src,tests}/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"{src,tests}/**/*.{ts,tsx,js,jsx}\" --fix",
    "prebuild": "rimraf ./dist/*",
    "test": "jest --coverage"
  },
  "peerDependencies": {
    "@babel/core": "^7.0",
    "webpack": "^4.0",
    "webpack-dev-server": "^3.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.0",
    "@babel/plugin-proposal-optional-chaining": "^7.12.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/preset-env": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@loadable/babel-plugin": "^5.13.0",
    "babel-loader": "^8.2.0",
    "babel-merge": "^3.0.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^5.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "dotenv-webpack": "^1.8.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "lodash-webpack-plugin": "^0.11.6",
    "terser-webpack-plugin": "^4.1.0",
    "webpack-merge": "^5.1.0",
    "webpack-subresource-integrity": "^1.4.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@types/compression-webpack-plugin": "^4.0.0",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/dotenv-webpack": "^1.8.0",
    "@types/lodash-webpack-plugin": "^0.11.4",
    "@types/terser-webpack-plugin": "^4.0.0",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-subresource-integrity": "^1.2.0",
    "@types/yargs": "^16.0.0",
    "babel-plugin-emotion": "^11.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "webpack": "^4.46.0",
    "webpack-dev-server": "^3.11.0"
  }
}
